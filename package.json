    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint src/**/*.ts"
  },
  "bin": {
    "odata-metadata-to-typescript-interfaces": "./lib/cli.js",
    "omtti": "./lib/cli.js"
  },
  "main": "lib/cli.js",
  "typings": "lib/cli.d.ts"
